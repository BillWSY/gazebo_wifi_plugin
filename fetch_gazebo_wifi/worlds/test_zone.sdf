<?xml version="1.0" ?>
<sdf version="1.4">
  <world name="default">
    <include>
      <uri>model://ground_plane</uri>
    </include>
    <include>
      <uri>model://sun</uri>
    </include>
    <include>
      <uri>model://test_zone</uri>
    </include>
    <include>
      <uri>model://cafe_table</uri>
      <name>table1</name>
      <pose>4.05 3 0 0 0 1.57</pose>
    </include>
    <include>
      <uri>model://cafe_table</uri>
      <name>table2</name>
      <pose>-3 5 0 0 0 1.57</pose>
    </include>
    <include>
      <uri>model://demo_cube</uri>
      <pose>3.8 3.15 0.83 0 0 0</pose>
    </include>

    <model name="box1">
      <link name="link">
        <pose>2 4 0.5 0 0 0</pose>

        <collision name="box_collision">
          <geometry>
            <box>
              <size>0.1 0.1 0.1</size>
            </box>
          </geometry>
        </collision>

        <visual name="visual">
          <geometry>
            <box>
              <size>0.1 0.1 0.1</size>
            </box>
          </geometry>
        </visual>

        <sensor name="cogrob_wireless_transmitter" type="wireless_transmitter">
          <plugin name="wifi_router_plugin" filename="libwifirouter.so"/>
          <always_on>1</always_on>
          <update_rate>1</update_rate>
          <visualize>true</visualize>
          <transceiver>
            <essid>CogRob_1</essid>
            <frequency>2442.0</frequency>
            <power>14.5</power>
            <gain>2.6</gain>
          </transceiver>
        </sensor>

      </link>
    </model>

    <model name="box2">
      <link name="link">
        <pose>2 2 0.5 0 0 0</pose>

        <collision name="box_collision">
          <geometry>
            <box>
              <size>0.1 0.1 0.1</size>
            </box>
          </geometry>
        </collision>

        <visual name="visual">
          <geometry>
            <box>
              <size>0.1 0.1 0.1</size>
            </box>
          </geometry>
        </visual>

        <sensor name="cogrob_wireless_transmitter" type="wireless_transmitter">
          <plugin name="wifi_router_plugin" filename="libwifirouter.so"/>
          <always_on>1</always_on>
          <update_rate>1</update_rate>
          <visualize>true</visualize>
          <transceiver>
            <essid>CogRob_2</essid>
            <frequency>2440.0</frequency>
            <power>10.0</power>
            <gain>3</gain>
          </transceiver>
        </sensor>

      </link>
    </model>
  </world>
</sdf>
